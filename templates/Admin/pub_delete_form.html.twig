<form method="post" action="{{ path('app_publication_delete', {'id_p': publication.id_p}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ publication.id_p) }}">
    <button class="btn">Supprimer</button>
</form>
